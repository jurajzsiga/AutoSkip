{
  "manifest_version": 2,

  "name": "AutoSkip",
  "description": "Auto skips intros on Netflix",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.netflix.com/*"],
      "js": ["jquery-3.5.1.min.js", "netflix.js"]
    }
  ]
}
